{
	"name": "foxland",
	"version": "1.0.0",
	"description": "Foxland site with Eleventy",
	"author": "Sami Keijonen",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/samikeijonen/foxland-11ty"
	},
	"scripts": {
		"build": "run-s clean build:assets build:site",
		"build:assets": "cross-env NODE_ENV=production webpack --mode production --config=.config/webpack.config.js",
		"build:site": "cross-env NODE_ENV=production eleventy",
		"clean": "rm -rf ./dist",
		"dev": "run-p dev:*",
		"dev:assets": "cross-env NODE_ENV=development webpack --watch --config=.config/webpack.config.js",
		"dev:site": "cross-env NODE_ENV=development eleventy --serve",
		"lint:css": "stylelint './src/css/**/*.css'",
		"lint:js": "eslint './src/js/**/*.js'"
	},
	"dependencies": {
		"@11ty/eleventy": "^0.11.1",
		"@babel/core": "^7.13.8",
		"@babel/preset-env": "^7.13.9",
		"autoprefixer": "^10.2.5",
		"axios": "^0.21.1",
		"babel-loader": "^8.2.2",
		"cssnano": "^4.1.10",
		"flat-cache": "^3.0.4",
		"html-minifier": "^4.0.0",
		"npm-run-all": "^4.1.5",
		"webpack": "^5.24.4",
		"webpack-cli": "^4.5.0",
		"webpack-manifest-plugin": "^3.0.0"
	},
	"devDependencies": {
		"@10up/eslint-config": "^2.1.1",
		"@foxland/stylelint-config": "^1.3.0",
		"@foxland/webpack-tiny-helpers": "^1.1.1",
		"@wordpress/eslint-plugin": "^8.0.2",
		"cross-env": "^7.0.3",
		"css-loader": "^5.1.1",
		"eslint": "^7.7.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-airbnb-base": "^14.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsdoc": "^30.3.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.6",
		"eslint-plugin-react-hooks": "^4.1.0",
		"luxon": "^1.25.0",
		"mini-css-extract-plugin": "^1.3.9",
		"postcss-editor-styles": "^0.3.0",
		"postcss-import": "^14.0.0",
		"postcss-loader": "^5.1.0",
		"postcss-mixins": "^7.0.3",
		"postcss-nested": "^5.0.5",
		"postcss-preset-env": "^6.7.0",
		"prettier": "^2.1.1",
		"stylelint": "^13.12.0"
	}
}
